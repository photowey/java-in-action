<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress files="[\\/].*(package-info)\.java" checks="RegexpOnFilename"/>
    <suppress files=".*[\\/]printer([\\/])?.*" checks="OperatorWrap"/>
    <suppress files=".*[\\/]grammar([\\/])?.*" checks="Regexp"/>
    <suppress files=".*[\\/]grammar([\\/])?.*" checks="UncommentedMain"/>
    <suppress files=".*[\\/]grammar([\\/])?.*" checks="LocalVariableName"/>
    <suppress files=".*[\\/]grammar([\\/])?.*" checks="MethodLength"/>
    <suppress files=".*[\\/]grammar([\\/])?.*" checks="WhitespaceAround"/>
</suppressions>
